    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-slick": "^0.23.13",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.7.4"
  },
  "resolutions": {
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.14"
  }
